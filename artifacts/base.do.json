{
    "$schema": "https://raw.githubusercontent.com/F5Networks/f5-declarative-onboarding/master/src/schema/latest/remote.schema.json",
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "Common": {
        "class": "Tenant",
        "System": {
            "class": "System",
            "hostname": "devCloud01.benlab.io",
            "consoleInactivityTimeout": 1200,
            "mgmtDhcpEnabled": true,
            "autoCheck": true,
            "autoPhonehome": false
        },
        "Authentication": {
            "class": "Authentication",
            "fallback": true,
            "remoteUsersDefaults": {
                "role": "admin",
                "terminalAccess": "tmsh"
            },
            "radius": {
                "servers": {
                    "primary": {
                        "secret": "",
                        "server": "10.200.244.1"
                    }
                }
            }
        },
        "ManagementIp_IPv4": {
            "class": "ManagementIp",
            "remark": "configured-statically",
            "address": "10.200.244.110/24"
        },
        "Provision": {
            "class": "Provision",
            "apm": "nominal",
            "avr": "nominal",
            "ltm": "nominal"
        },
        "NTP": {
            "class": "NTP",
            "timezone": "US/Central"
        },
        "DNS": {
            "class": "DNS",
            "nameServers": [
                "192.168.200.7",
                "192.168.200.8"
            ],
            "search": [
                "benlab.io"
            ]
        },
        "internal": {
            "class": "VLAN",
            "tag": 4094,
            "interfaces": [
                {
                    "name": "1.0",
                    "tagged": false
                }
            ]
        },
        "Analytics": {
            "class": "Analytics"
        },
        "default": {
            "class": "ManagementRoute",
            "remark": "configured-statically",
            "gw": "10.200.244.1",
            "network": "default"
        },
        "/Common/comm-public": {
            "source": "default",
            "class": "SnmpCommunity",
            "name": "public"
        },
        "HTTPD": {
            "class": "HTTPD",
            "authPamIdleTimeout": 12000
        }
    }
}
