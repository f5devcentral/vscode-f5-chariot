{
	"name": "vscode-f5-chariot",
	"displayName": "F5 ACC Chariot",
	"description": "(ACC) AS3 Configuration Converter integration",
	"version": "1.11.0",
	"license": "Apache-2.0",
	"publisher": "F5DevCentral",
	"icon": "images/f5.png",
	"galleryBanner": {
		"color": "#DCDCDC",
		"theme": "light"
	},
	"homepage": "https://github.com/f5devcentral/vscode-f5-chariot/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/f5devcentral/vscode-f5-chariot"
	},
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "f5.chariot.convert",
				"title": "Convert with ACC",
				"category": "F5"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "f5.chariot.convert",
					"group": "chariot"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^14.14.37",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"eslint": "^7.11.0",
		"mocha": "^8.3.2",
		"ts-node": "^9.1.1",
		"typescript": "^4.0.2"
	},
	"dependencies": {
		"f5-conx-core": "github:dumpysquare/f5-conx-core"
	}
}
