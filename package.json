{
	"name": "vscode-f5-chariot",
	"displayName": "F5 ACC Chariot",
	"description": "(ACC) AS3 Configuration Converter integration",
	"version": "1.22.0",
	"license": "Apache-2.0",
	"publisher": "F5DevCentral",
	"icon": "images/f5.png",
	"galleryBanner": {
		"color": "#DCDCDC",
		"theme": "light"
	},
	"homepage": "https://github.com/f5devcentral/vscode-f5-chariot/blob/main/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/f5devcentral/vscode-f5-chariot"
	},
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:f5.chariot.convertAS3",
		"onCommand:f5.chariot.convertDO"
	],
	"main": "./out/extension.js",
	"files": [
		"out/**",
		"images/**",
		"node_modules/**"
	],
	"contributes": {
		"commands": [
			{
				"command": "f5.chariot.convertAS3",
				"title": "Convert to AS3 with ACC",
				"category": "F5"
			},
			{
				"command": "f5.chariot.convertDO",
				"title": "Convert to DO with ACC",
				"category": "F5"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "f5.chariot.convertAS3",
					"group": "chariot"
				},
				{
					"command": "f5.chariot.convertDO",
					"group": "chariot"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"package": "vsce package --no-dependencies",
		"lint": "eslint . --ext .ts,.tsx",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/tests/runTests.js",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "^17.0.12",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"eslint": "^8.57.0",
		"mocha": "^11.7.5",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.4.1"
	},
	"dependencies": {
		"f5-automation-config-converter": "file:f5-automation-config-converter-1.126.0.tgz",
		"f5-conx-core": "^1.2.0"
	},
	"overrides": {
		"f5-automation-config-converter": {
			"ajv": "8.17.1"
		},
		"semver": "7.7.3"
	},
	"eslintConfig": {
		"root": true,
		"parser": "@typescript-eslint/parser",
		"plugins": [
			"@typescript-eslint"
		],
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended"
		],
		"rules": {
			"semi": [
				2,
				"always"
			],
			"@typescript-eslint/no-unused-vars": 0,
			"@typescript-eslint/no-explicit-any": 0,
			"@typescript-eslint/explicit-module-boundary-types": 0,
			"@typescript-eslint/no-non-null-assertion": 0
		}
	}
}
