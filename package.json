{
	"name": "vscode-f5-chariot",
	"displayName": "F5 ACC Chariot",
	"description": "(ACC) AS3 Configuration Converter integration",
	"version": "1.16.2",
	"license": "Apache-2.0",
	"publisher": "F5DevCentral",
	"icon": "images/f5.png",
	"galleryBanner": {
		"color": "#DCDCDC",
		"theme": "light"
	},
	"homepage": "https://github.com/f5devcentral/vscode-f5-chariot/blob/main/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/f5devcentral/vscode-f5-chariot"
	},
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:f5.chariot.convertAS3",
		"onCommand:f5.chariot.convertDO"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "f5.chariot.convertAS3",
				"title": "Convert to AS3 with ACC",
				"category": "F5"
			},
			{
				"command": "f5.chariot.convertDO",
				"title": "Convert to DO with ACC",
				"category": "F5"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "f5.chariot.convertAS3",
					"group": "chariot"
				},
				{
					"command": "f5.chariot.convertDO",
					"group": "chariot"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/tests/runTests.js",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "^14.14.37",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"eslint": "^7.11.0",
		"mocha": "^9.0.1",
		"ts-node": "^9.1.1",
		"typescript": "^4.0.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"f5-as3-config-converter": "file:f5-as3-config-converter-v1.16.0-source.tgz",
		"f5-conx-core": "^0.11.0"
	}
}
