{
	"name": "vscode-f5-chariot",
	"displayName": "F5 Chariot",
	"description": "(ACC) AS3 Configuration Converter integration",
	"version": "0.5.0",
	"publisher": "F5DevCentral",
	"repository": {
		"type": "git",
		"url": "https://github.com/f5devcentral/vscode-f5-chariot"
	},
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:f5.chariot.convert",
		"onCommand:f5.chariot.selectImage",
		"onCommand:f5.chariot.settings"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "f5-chariot",
			"properties": {
				"f5.chariot.dockerImage": {
					"type": "string",
					"description": "docker image to run",
					"default": "f5-appsvcs-charon:1.8.0"
				},
				"f5.chariot.outFileName": {
					"type": "string",
					"description": "output file name",
					"default": "converted.as3.json"
				},
				"f5.chariot.fullCommand": {
					"type": "string",
					"description": "Full docker command: to allow full control of how docker is executed.  Default=blank.  This setting will superceed all other docker command attributes.  Be sure output file name in this setting matches the \"Out File Name\" setting.  One is for docker command execution, the other is for vscode to open the file in an editor.",
					"default": "",
					"examples": [
						"docker run --rm -v \"/Users/SomeUser/acc\":/app/data f5-appsvcs-charon:1.8.0 -o data/converted.as3.json -c data/toConvert.conf --unsupported --unsupported-objects unSupported.json"
					]
				}
			}
		},
		"commands": [
			{
				"command": "f5.chariot.convert",
				"title": "Chariot Convert",
				"category": "F5"
			},
			{
				"command": "f5.chariot.selectImage",
				"title": "Chariot Select Image",
				"category": "F5"
			},
			{
				"command": "f5.chariot.settings",
				"title": "Chariot Settings",
				"category": "F5"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "f5.chariot.convert",
					"group": "chariot"
				},
				{
					"command": "f5.chariot.selectImage",
					"group": "chariot"
				}
			],
			"commandPalette": [
				{
					"command": "f5.chariot.convert",
					"when": "none"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/dockerode": "^3.2.2",
		"@types/node": "^12.12.68",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^3.10.1",
		"@typescript-eslint/parser": "^3.10.1",
		"eslint": "^7.11.0",
		"typescript": "^4.0.2"
	},
	"dependencies": {
		"dockerode": "^3.2.1"
	}
}
